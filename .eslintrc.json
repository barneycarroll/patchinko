{
  "parserOptions": {
    "ecmaVersion": 3,
    "sourceType": "script",
    "ecmaFeatures": {}
  },
  "rules": {
    "no-console": 2,
    "no-debugger": 2,
    "no-undef": 2,
    "no-unreachable": 2,
    "no-unused-vars": 2,
    "no-unused-labels": 2
  },
  "env": {
    "commonjs": true,
    "browser": true
  }
}
